{
    "default_path": "/usr/lib/rust/",
    "default_extension": "so",
    "timeout": 2000,
    "extns": [
        {
            "path": "libthunder",
            "symbols": [
                {
                    "id": "ripple:channel:device:thunder",
                    "uses": [
                        "config"
                    ],
                    "fulfills": [
                        "device_info",
                        "window_manager",
                        "browser",
                        "wifi",
                        "device_events",
                        "remote_accessory"
                    ]
                }
            ]
        },
        {
            "path": "libdistributor_general",
            "symbols": [
                {
                    "id": "ripple:channel:distributor:general",
                    "uses": [
                        "config"
                    ],
                    "fulfills": [
                        "permissions",
                        "advertising",
                        "privacy_settings",
                        "metrics",
                        "discovery",
                        "media_events"
                    ]
                }
            ]
        },
        {
            "path": "libdistributor_liberty",
            "symbols": [
                {
                    "id": "ripple:channel:distributor:liberty",
                    "uses": [
                        "config",
                        "internal"
                    ],
                    "fulfills": [
                        "advertising",
                        "local.storage"
                    ]
                }
            ]
        }
    ],
    "required_contracts": [
        "rpc",
        "lifecycle_management",
        "device_info",
        "window_manager",
        "browser",
        "permissions",
        "wifi",
        "device_events",
        "remote_accessory",
        "advertising",
        "privacy_settings",
        "metrics",
        "discovery",
        "media_events"
    ],
    "rpc_aliases": {
        "device.model": ["custom.model"]
    }
}